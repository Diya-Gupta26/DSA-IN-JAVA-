public class Main
{
	static void bubble_sort(int array[]){
	    
	    	for(int i=0; i<array.length-1; i++) 
		{
			for(int j=0; j<array.length-(i+1);j++){
			    if(array[j]> array[j+1])
			{
			
			int temp=array[j];
			array[j]=array[j+1];
			array[j+1]=temp;
			
	} 
			}
		}
	}

	public static void main(String[] args) {
		
		int array[] = {10,5,7,3,6,1};
		
		bubble_sort(array);
	
		for(int x: array)
		System.out.print(x+" ");
	}
}
